<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddTestController">
   <children>
      <Pane layoutX="10.0" layoutY="11.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <GridPane layoutX="129.0" layoutY="18.0" prefHeight="282.0" prefWidth="343.0">
               <children>
                  <Label text="Patient ID :" />
                  <Label text="Test Name :" GridPane.rowIndex="1" />
                  <Label text="Test Date :" GridPane.rowIndex="2" />
                  <TextField fx:id="PID" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="187.0" GridPane.columnIndex="1" />
                  <ComboBox fx:id="Test_Name" onMouseClicked="#TestSelect" prefHeight="25.0" prefWidth="186.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <DatePicker fx:id="Test_Date" prefHeight="25.0" prefWidth="186.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Refered By :" GridPane.rowIndex="3" />
                  <TextField fx:id="REF_BY" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="186.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="181.0" minWidth="10.0" prefWidth="113.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="261.0" minWidth="10.0" prefWidth="230.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
            <Button fx:id="Savebtn" layoutX="240.0" layoutY="326.0" mnemonicParsing="false" onAction="#btnSaveTest" text="Save" />
            <Button fx:id="Updatebtn" layoutX="309.0" layoutY="326.0" mnemonicParsing="false" onAction="#btnUpdateTest" text="Update" />
         </children>
      </Pane>
   </children>
</AnchorPane>
