<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.BedController">
   <children>
      <Pane layoutX="-67.0" layoutY="1.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <GridPane layoutX="128.0" layoutY="40.0" prefHeight="241.0" prefWidth="327.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="159.0" minWidth="10.0" prefWidth="122.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="213.0" minWidth="10.0" prefWidth="205.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Bed No :" />
                  <Label text="Status :" GridPane.rowIndex="1" />
                  <Label text="Employee Assigned :" GridPane.rowIndex="2" />
                  <TextField fx:id="Bedno" maxWidth="-Infinity" prefHeight="25.0" prefWidth="191.0" GridPane.columnIndex="1" />
                  <ComboBox fx:id="Status" onMouseClicked="#StatusSelect" prefHeight="25.0" prefWidth="190.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <ComboBox fx:id="Ename" onMouseClicked="#EnameSelect" prefHeight="25.0" prefWidth="191.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <Button fx:id="Updatebtn" layoutX="257.0" layoutY="330.0" mnemonicParsing="false" onAction="#btnUpdateBed" text="Update" />
         </children>
      </Pane>
   </children>
</AnchorPane>
